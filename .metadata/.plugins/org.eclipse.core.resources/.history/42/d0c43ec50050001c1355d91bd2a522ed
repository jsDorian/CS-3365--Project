package application;

public class Item {
	public String name;
	public int price;
	public int amount;
	public int bulk;
	public int id;
	
	public Item(String name, int price, int amount, int bulk) {
		this.name = name;
		this.price = price;
		this.amount = amount;
		this.bulk = bulk;
	}
	
	public Item(String name, int price, int amount, int bulk, int id) {
		this.name = name;
		this.price = price;
		this.amount = amount;
		this.bulk = bulk;
		this.id = id;
	}
}
